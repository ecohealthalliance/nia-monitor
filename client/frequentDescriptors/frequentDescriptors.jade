template(name="frequentDescriptors")
  table#frequentDescriptorsTable.table.table-striped
    tbody
      each frequentDescriptors
        tr
          if count
            td.frequentDescriptorsTableRow(data-agentname=name)
              div.fmia-word= selText
            td
              =count
          else
            span.no-priors.col-md-12 No Frequent Descriptors
