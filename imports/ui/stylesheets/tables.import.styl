.table td
  border-top 1px solid $l-gray
  border-bottom 1px solid $l-gray
  padding 20px

hr
  position relative
  background-color $secondary
  height 3px
  width 100%
  bottom 53px
  border 0

.hr-padding
  padding-left 0px
  padding-right 0px

.power-bars
  text-align right

.power-bar
  background-color $primary
  display inline-table
  width 1.3em
  height .2em
  vertical-align middle
  margin-left .5em
